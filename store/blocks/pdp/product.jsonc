    "children": []
  },

  "header-layout.mobile#empty": {
    "children": []
  },

  // DECLARA A CONDIÇÃO DO HEADER NORMAL (SE DIFERENTE DESTAS MARCAS, RENDERIZAR TOPO NORMAL)

  "condition-layout.product#headercommon": {
    "props": {
      "conditions": [{
          "subject": "brandId",
          "arguments": {
            "id": ["2000000"]
          }
        },
        {
          "subject": "brandId",
          "arguments": {
            "id": ["2000001"]
          }
        },
        {
          "subject": "brandId",
          "arguments": {
            "id": ["2000002"]
          }
        },
        {
          "subject": "brandId",
          "arguments": {
            "id": ["2000003"]
          }
        },
        {
          "subject": "brandId",
          "arguments": {
            "id": ["1097"]
          }
        }
      ],
      "matchType": "none",
      "Then": "header#common"
    }
  },

