  // DEFINIR HEADER NORMAL

    "header#common": {
      "blocks": ["header-layout.desktop#common", "header-layout.mobile#common"]
    },
    "header.full#common": {
      "blocks": ["header-layout.desktop#common", "header-layout.mobile#common"]
    },
    "header-layout.desktop#common": {
      "children": [
        "flex-layout.row#1-desktop-common",
        "sticky-layout#desktop-common"
      ]
    },
  
    "flex-layout.row#1-desktop-common": {
